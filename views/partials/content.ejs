<% content.onPage.forEach(item=> { %>
	<div class="subTitleText">
		<%= item.title %>
	</div>
	<% if(item.contentType==='TEXT' ) { %>
		<div class="contentText">
			<%= item.content %>
		</div>
		<% } %>
			<% if(item.contentType==='IMAGE' ) { %>
				<div class="contentImage">
					<image src="<%= item.content %>"></image>
				</div>
				<% } %>
					<% }); %>